# App OAuth credentials
WORDLE_PUBLIC_OAUTH_CLIENT_ID=oauth2-client-id-here
WORDLE_OAUTH_CLIENT_SECRET=oauth2-client-secret-here

# JWT Secret for signing tokens
WORDLE_JWT_SECRET=very_secure_jwt_secret_here


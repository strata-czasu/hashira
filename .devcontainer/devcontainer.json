{
  "name": "Bun",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "features": {
    "ghcr.io/devcontainers/features/github-cli": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.vscode-typescript-next",
        "oven.bun-vscode",
        "biomejs.biome",
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "eamodio.gitlens",
        "Prisma.prisma"
      ]
    }
  }
}
